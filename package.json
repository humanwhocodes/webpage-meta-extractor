{
  "name": "@humanwhocodes/webpage-meta-extractor",
  "version": "0.5.2",
  "description": "Extracts Open Graph, Twitter Card, and other meta tags from HTML documents.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "!(*.js)": [
      "prettier --write --ignore-unknown"
    ]
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/nzakas"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/ tests/",
    "fmt": "prettier --write .",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "mocha tests/**/*.{js,mjs} --exit --forbid-only",
    "test:jsr": "npx jsr@latest publish --dry-run",
    "test:attw": "attw --pack --profile esm-only"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/humanwhocodes/webpage-meta-extractor.git"
  },
  "keywords": [],
  "author": "Nicholas C. Zaks",
  "license": "Apache-2.0",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "@eslint/js": "^9.0.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "^22.9.0",
    "eslint": "^9.0.0",
    "jsdom": "^26.1.0",
    "lint-staged": "15.4.3",
    "mocha": "^11.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "yorkie": "2.0.0"
  }
}
